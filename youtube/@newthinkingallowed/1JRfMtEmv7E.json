[{"startMs":0,"durationMs":2000,"subtitle":"[Music]"},{"startMs":4500,"durationMs":6120,"subtitle":"Thinking Allowed Conversations on the leading edge of knowledge"},{"startMs":10720,"durationMs":10440,"subtitle":"and discovery with psychologist Jeffrey Mishlove."},{"startMs":21160,"durationMs":1000,"subtitle":"Hello and welcome."},{"startMs":22160,"durationMs":4980,"subtitle":"I'm Jeffrey Mishlove and I'm Jeffrey Mishlove, too."},{"startMs":27140,"durationMs":8020,"subtitle":"This is my fifth experiment in what seems to be evolving into what I think might be"},{"startMs":35160,"durationMs":1840,"subtitle":"a new art form."},{"startMs":37000,"durationMs":4520,"subtitle":"It is and it is not poetry."},{"startMs":41520,"durationMs":4400,"subtitle":"It is and it is not meditation."},{"startMs":45920,"durationMs":3280,"subtitle":"It's something of a blend between the two."},{"startMs":49200,"durationMs":4259,"subtitle":"I'm going to try and formalize it as I move"},{"startMs":53471,"durationMs":4369,"subtitle":"forward by limiting each line to no more than"},{"startMs":57840,"durationMs":9120,"subtitle":"five words and by keeping it to precisely 60 lines that are developed over the course"},{"startMs":66960,"durationMs":3080,"subtitle":"of about a week of meditation."},{"startMs":70040,"durationMs":3720,"subtitle":"In effect, it's my own stream of consciousness."},{"startMs":73760,"durationMs":4400,"subtitle":"It's my own chain of association."},{"startMs":78160,"durationMs":7960,"subtitle":"And it is my hope and my expectation that by entering into a meditative state myself"},{"startMs":86120,"durationMs":6400,"subtitle":"as I recite the words that I have written down while I was meditating – well, when"},{"startMs":92520,"durationMs":4246,"subtitle":"I came out of meditation to write those words"},{"startMs":96778,"durationMs":3702,"subtitle":"– that I'll be able to transmit to those"},{"startMs":100480,"durationMs":4509,"subtitle":"who are sensitive and able to receive a kind"},{"startMs":105001,"durationMs":4119,"subtitle":"of awareness, a glimpse, a window, a door"},{"startMs":109120,"durationMs":6440,"subtitle":"perhaps into my own mind for what it's worth for our viewers."},{"startMs":115560,"durationMs":7560,"subtitle":"And now I'll begin."},{"startMs":123120,"durationMs":14520,"subtitle":"Living up."},{"startMs":137640,"durationMs":25680,"subtitle":"Transcendence."},{"startMs":163320,"durationMs":13320,"subtitle":"Here and now."},{"startMs":176640,"durationMs":20200,"subtitle":"My body."},{"startMs":196840,"durationMs":16800,"subtitle":"Here and then."},{"startMs":213640,"durationMs":21240,"subtitle":"My soul."},{"startMs":234880,"durationMs":21880,"subtitle":"All spaces and times."},{"startMs":256760,"durationMs":26320,"subtitle":"My spirit."},{"startMs":283080,"durationMs":24960,"subtitle":"Time loops."},{"startMs":308040,"durationMs":12040,"subtitle":"In loop galaxies."},{"startMs":320080,"durationMs":19640,"subtitle":"Spirals everywhere."},{"startMs":339720,"durationMs":17040,"subtitle":"Strange attractors."},{"startMs":356760,"durationMs":21120,"subtitle":"Reality tunnels."},{"startMs":377880,"durationMs":16160,"subtitle":"Storm holes in cosmic soil."},{"startMs":394040,"durationMs":19040,"subtitle":"There all at once."},{"startMs":413080,"durationMs":18400,"subtitle":"Happy and sad."},{"startMs":431480,"durationMs":16480,"subtitle":"Peaceful and vicious."},{"startMs":447960,"durationMs":13280,"subtitle":"Fusion."},{"startMs":461240,"durationMs":16160,"subtitle":"Best and worst."},{"startMs":477400,"durationMs":20560,"subtitle":"Wholeness begets healing."},{"startMs":497960,"durationMs":15680,"subtitle":"Sunshine mud."},{"startMs":513640,"durationMs":15920,"subtitle":"Hop, skip, jump, turn."},{"startMs":529560,"durationMs":19680,"subtitle":"Slip."},{"startMs":549240,"durationMs":13520,"subtitle":"Days of light."},{"startMs":562760,"durationMs":18960,"subtitle":"Oblivion nirvana."},{"startMs":581720,"durationMs":16080,"subtitle":"A point of awareness."},{"startMs":597800,"durationMs":16400,"subtitle":"With heart."},{"startMs":614200,"durationMs":16480,"subtitle":"Best version of myself."},{"startMs":630680,"durationMs":14480,"subtitle":"Total embodiment."},{"startMs":645160,"durationMs":16960,"subtitle":"Two brains."},{"startMs":662120,"durationMs":20400,"subtitle":"Embrace paradox."},{"startMs":682520,"durationMs":24640,"subtitle":"Edge of hilarity."},{"startMs":707160,"durationMs":18320,"subtitle":"God created the universe."},{"startMs":725480,"durationMs":18480,"subtitle":"Let there be light."},{"startMs":743960,"durationMs":15520,"subtitle":"I am a meditator."},{"startMs":759480,"durationMs":19680,"subtitle":"I am."},{"startMs":779160,"durationMs":14560,"subtitle":"Maha Kali."},{"startMs":793720,"durationMs":16960,"subtitle":"Great messiah."},{"startMs":810680,"durationMs":18880,"subtitle":"Loving heart of Jesus."},{"startMs":829560,"durationMs":19520,"subtitle":"Cosmic Christ approaching."},{"startMs":849080,"durationMs":20960,"subtitle":"No time, no space."},{"startMs":870040,"durationMs":19520,"subtitle":"All is none."},{"startMs":889560,"durationMs":20960,"subtitle":"Magic words."},{"startMs":910520,"durationMs":16480,"subtitle":"From the heart."},{"startMs":927000,"durationMs":17840,"subtitle":"With wings."},{"startMs":944840,"durationMs":20640,"subtitle":"Firm is emptiness."},{"startMs":965480,"durationMs":18960,"subtitle":"Radiant light."},{"startMs":984440,"durationMs":20800,"subtitle":"Legacy of wisdom."},{"startMs":1005240,"durationMs":16960,"subtitle":"Everything is nothing."},{"startMs":1022200,"durationMs":16080,"subtitle":"Astral flight."},{"startMs":1038280,"durationMs":20960,"subtitle":"Astral healing light."},{"startMs":1059240,"durationMs":20080,"subtitle":"Dancing colors."},{"startMs":1079320,"durationMs":21680,"subtitle":"Dancing scenarios."},{"startMs":1101000,"durationMs":16800,"subtitle":"Probability waves."},{"startMs":1117800,"durationMs":23120,"subt